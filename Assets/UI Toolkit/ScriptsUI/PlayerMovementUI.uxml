                    <ui:Label display-tooltip-when-elided="true" name="Line39" enable-rich-text="true" text="        // Get the player’s speed (the absolute value of their x velocity)" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line40" enable-rich-text="true" text="        float playerSpeed =  Mathf.Abs(playerBody.velocity.x);" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line41" enable-rich-text="true" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line42" enable-rich-text="true" text="       // If the player is inputting a direction, apply speed to the character" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line43" enable-rich-text="true" text="        if (inputDirection.magnitude &gt; 0.01f){" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line44" enable-rich-text="true" text="             // This function caps our force so we don’t speed the player up too much" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line45" enable-rich-text="true" text="&#x9; float moveForce = getMoveForce();" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line46" enable-rich-text="true" text="             // Apply that force to the player in the direction they’re inputting" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line47" enable-rich-text="true" text="             playerBody.AddForce(inputDirection * moveForce, ForceMode2D.Force);" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line48" enable-rich-text="true" text="               // If the player is inputting to the right, tell the renderer to flip the player’s sprite" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line49" enable-rich-text="true" text="              playerRenderer.flipX = (inputDirection &gt; 0)" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line50" enable-rich-text="true" text="            }" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line51" enable-rich-text="true" text="        }" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line52" enable-rich-text="true" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line53" enable-rich-text="true" text="        //If there is no input but the character is moving, apply friction" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line54" enable-rich-text="true" text="        else if (Mathf.Abs(playerBody.velocity.x) &gt; 0.1f){" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line55" enable-rich-text="true" text="            // Dont apply more friction than the player’s current speed" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line56" enable-rich-text="true" text="            // Applying too much friction would push the player backwards" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line57" enable-rich-text="true" text="            float amount = Mathf.Min(playerSpeed, friction);" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line58" enable-rich-text="true" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line59" enable-rich-text="true" text="            // Set the friction in the opposite direction of the player’s movement" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line60" enable-rich-text="true" text="            amount *= Mathf.Sign(playerBody.velocity.x);" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line61" enable-rich-text="true" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line62" enable-rich-text="true" text="            // Apply the friction on the horizontal axis (Vector2.right)" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line63" enable-rich-text="true" text="            playerBody.AddForce(Vector2.right * amount, ForceMode2D.Impulse);" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line64" enable-rich-text="true" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line65" enable-rich-text="true" text="        }" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line66" enable-rich-text="true" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line67" enable-rich-text="true" text="         //Update the animator with the current horizontal speed" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line68" enable-rich-text="true" text="        spriteAnimator.SetFloat(&quot;Horizontal Speed&quot;, playerSpeed);" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
                    <ui:Label display-tooltip-when-elided="true" name="Line69" enable-rich-text="true" text="    }" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(125, 125, 125); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
