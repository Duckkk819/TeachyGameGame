                    <ui:Label display-tooltip-when-elided="true" name="Line37" enable-rich-text="true" text="    // Move the player in the set direction" style="background-color: rgba(255, 255, 255, 0); font-size: 20px; color: rgb(92, 148, 50); margin-left: 0; margin-top: 0; height: 28px; width: 1100px;" />
